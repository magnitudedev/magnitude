{
    "$schema": "https://mintlify.com/docs.json",
    "name": "Magnitude",
    "theme": "maple",
    "description": "Open source AI agents for web testing",
    "logo": "/logo.svg",
    "favicon": "/favicon.png",
    "icons": {
        "library": "lucide"
    },
    "colors": {
        "primary": "#0369a1",
        "light": "#0ea5e9",
        "dark": "#075985"
    },
    "navbar": {
        "links": [
            {
                "label": "Community",
                "href": "https://discord.gg/VcdpMh9tTy"
            }
        ],
        "primary": {
            "type": "button",
            "label": "GitHub",
            "href": "https://github.com/magnitudedev/magnitude"
        }
    },
    "navigation": {
        "global": {
            "anchors": [
                {
                    "anchor": "GitHub",
                    "icon": "github",
                    "href": "https://github.com/magnitudedev/magnitude"
                },
                {
                    "anchor": "Community",
                    "icon": "discord",
                    "href": "https://discord.gg/VcdpMh9tTy"
                },
                {
                    "anchor": "Contact",
                    "icon": "mail",
                    "href": "mailto:founders@magnitude.run"
                }
            ]
        },
        "dropdowns": [
            {
                "dropdown": "Documentation",
                "icon": "book",
                "groups": [
                    {
                        "group": "Getting Started",
                        "pages": [
                            "getting-started/introduction",
                            "getting-started/quickstart"
                        ]
                    },
                    {
                        "group": "Core Concepts",
                        "pages": [
                            "core-concepts/building-test-cases",
                            "core-concepts/running-tests"
                        ]
                    },
                    {
                        "group": "Customizing",
                        "pages": [
                            "customizing/configuration",
                            "customizing/self-hosting-moondream"
                        ]
                    },
                    {
                        "group": "Integrations",
                        "pages": [
                            "integrations/playwright",
                            "integrations/github-actions",
                            "integrations/mcp"
                        ]
                    },
                    {
                        "group": "Reference",
                        "pages": [
                            "reference/llm-configuration",
                            "reference/test-cases",
                            "reference/test-steps",
                            "reference/test-groups",
                            "reference/cli"
                        ]
                    }
                ]
            },
            {
                "dropdown": "Contributing",
                "icon": "wrench",
                "groups": [
                    {
                        "group": "Contributing",
                        "pages": [
                            "contributing/introduction",
                            "contributing/setup",
                            "contributing/suggestions"
                        ]
                    }
                ]
            }
        ]
    },
    "search": {
        "prompt": "Search or ask..."
    },
    "seo": {
        "indexing": "all"
    },
    "contextual": {
        "options": [
            "copy",
            "view"
        ]
    }
}